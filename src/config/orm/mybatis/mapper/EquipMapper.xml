<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="EquipMapper">

	<insert id="addEquip">
		insert into equipment (id , phone , name , uId)
		values (#{id},#{phone},#{name},#{uId})
	</insert>

	<select id="selectEquip" resultType="Equipment">
		select * from equipment
		where id = #{id}
	</select>
	
</mapper>